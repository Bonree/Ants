<?xml version="1.0" encoding="UTF-8"?>
<schema>
<tables name="sdk_stat_cdn">
<!-- 内置 -->
<field desc="接入方式" name="access_model" source="accessModel" valueType="long" type="dimension"/>
<field desc="APP版本号" name="app_ver_id" source="appVerId" valueType="long" type="dimension"/>
<!-- 内置字段 需要name转id -->
<field desc="APPID" name="appid" source="appid" valueType="long" type="dimension"/>
<!-- 内置 -->
<field desc="CDN;缓存中获取的" name="cdn_id" source="cdnId" valueType="long" type="dimension"/>
<!-- 内置 -->
<field desc="地域" name="district" source="district" valueType="long" type="dimension"/>
<!-- 内置 -->
<field desc="域名" name="domaincode" source="domain" valueType="long" type="dimension"/>
<!-- 内置 -->
<field desc="运营商" name="netservice_id" source="netserviceId" valueType="long" type="dimension"/>
<field desc="错误类型:0:normal;1:http;2:dns;3:tcp;4:ssl;5:js;6:other" name="errorType" source="errorType" valueType="long" type="hidden"/>
<!-- 内置tcp+ssl+dns+response -->
<field desc="下载时间;（单位S）" name="down_time" source="lossDownTime" valueType="double" type="metric" expr="sumif$(errorType=0)"/>
<!-- 内置 -->
<field desc="下载大小;（单位KB）" name="down_size" source="lossDownSize" valueType="double" type="metric" expr="sumif$(errorType=0)"/>
<field desc="错误请求次数" name="error_num" source="errorNum" valueType="long" type="metric" expr="countif$(errorType>0)"/>
<field desc="总正确请求次数" name="request_num" source="requestNum" valueType="long" type="metric" expr="countif$(errorType=0)"/>
<!-- 0表示非慢请求 1表示慢请求 -->
<field desc="慢请求次数" name="slow_num" source="slowNum" valueType="long" type="metric" expr="sumif$(errorType=0)"/>
<field desc="整体性能" name="total_time" source="totalTime" valueType="double" type="metric" expr="sumif$(errorType=0)"/>
</tables>
</schema>
