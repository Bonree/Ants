<?xml version="1.0" encoding="UTF-8"?>
<schema>
<tables name="sdk_stat_crash">
<!-- 内置字段若crah不存在 取heade 需要name转id -->
<field desc="APP版本" name="app_ver_id" source="crashAppVerID" valueType="long" type="dimension"/>
<!-- 内置字段需要name转id -->
<field desc="APPID" name="appid" source="appid" valueType="long" type="dimension"/>
<!-- 内置字段通过band+model+patarn缓存转换为id -->
<field desc="品牌ID" name="brand_id" source="brandId" valueType="long" type="dimension"/>
<!-- 内置字段 通过根据appid 设备及堆栈信息转换成code -->
<field desc="崩溃问题ID" name="crash_event_id" source="eventCode" valueType="long" type="dimension"/>
<!-- 内置字段 通过crashtype keyfuction casueby组合而成 -->
<field desc="崩溃问题" name="crash_event" source="event" valueType="string" type="dimension"/>
<!-- 内置字段通过方法转换code -->
<field desc="崩溃类型id" name="crash_type_code" source="crashTypeCode" valueType="long" type="dimension"/>
<!-- 取值操作 长度截取 -->
<field desc="崩溃类型" name="crash_type" source="crashType" valueType="string" type="dimension"/>
<!-- 内置字段通过os缓存转换为id -->
<field desc="操作系统ID" name="os_id" source="osId" valueType="long" type="dimension"/>
<field desc="崩溃次数" name="crash_num" source="crash_num" valueType="long" type="metric" expr="sum$(1)"/>
<field desc="最早发生时间" name="first_time" source="monitorTime" valueType="timestamp" type="metric" expr="min"/>
<field desc="最晚发生时间" name="last_time" source="monitorTime" valueType="timestamp" type="metric" expr="max"/>
</tables>
</schema>
